{
  "name": "mediview-video-service",
  "version": "1.0.0",
  "description": "Secure video calling service for healthcare applications",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "webrtc",
    "video-call",
    "healthcare",
    "telemedicine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.29.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "socket.io": "^4.7.4",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/methods": "^1.1.4",
    "@types/node": "^24.1.0",
    "@types/socket.io": "^3.0.1",
    "@types/styled-components": "^5.1.34",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/mediview-video-service.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/mediview-video-service/issues"
